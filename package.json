{
  "name": "vovo-solana",
  "version": "1.0.0",
  "scripts": {
    "push":"git add . && git commit -m 'auto updates' && git push",
    "switch-localnet":"solana config set --url localhost",
    "switch-devnet":"solana config set --url devnet",
    "switch-testnet":"solana config set --url testnet",
    "switch-mainnet-beta":"solana config set --url mainnet-beta",
    "start-server": "solana-test-validator",
    "build": "cargo build-bpf --manifest-path=./programs/vovo/Cargo.toml --bpf-out-dir=dist",
    "clean": "cargo clean --manifest-path=./programs/vovo/Cargo.toml && rm -rf ./dist",
    "test": "cargo test-bpf --manifest-path=./programs/vovo/Cargo.toml"
  },

  "dependencies": {
    "@project-serum/anchor": "^0.11.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@pythnetwork/client": "^2.2.0",
    "@solana/spl-token": "^0.1.6",
    "black-scholes": "^1.1.0",
    "import-jsx": "^4.0.0",
    "ink": "^3.0.8",
    "moment": "^2.29.1",
    "react": "^17.0.2"
  }
}
